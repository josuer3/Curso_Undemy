@using Curso_Undemy.Models;
@model List<SucursalCLS>
   @{  
        ViewBag.Title = "Index";
    }

<h2>Index</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td width="25%">
                @Html.LabelFor(p => Model[0].iidsucursal)
            </td>
            <td width="25%">
                @Html.LabelFor(p => Model[0].nombre)
            </td>
            <td width="25%">
                @Html.LabelFor(p => Model[0].telefono)
            </td>
            <td width="25%">
                @Html.LabelFor(p => Model[0].email)
            </td>
        </tr>
    </thead>
    <tbody>

        llamando data de la lista
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.iidsucursal</td>
                <td>@item.nombre</td>
                <td>@item.telefono</td>
                <td>@item.email</td>
            </tr>
        }
    </tbody>

</table>

